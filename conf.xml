		<patternEntry>
			<name>login</name>
			<regex>sign(ed)?(\\s|_)?(in|out)|log(ged)?(\\s|_)?(in|out)</regex>
		</patternEntry>
		<patternEntry>
			<name>username</name>
			<regex>user(\\s|_)?(name|id)?</regex>
