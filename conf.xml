		<patternEntry>
			<name>input</name>
			<regex>number|price|quantity|qty\\s|zip\\s?code</regex>
		</patternEntry>
		<patternEntry>
			<name>clear</name>
			<regex>number|price|quantity|qty\\s|zip\\s?code</regex>
		</patternEntry>
	</tokenizerPatterns>
</conf>